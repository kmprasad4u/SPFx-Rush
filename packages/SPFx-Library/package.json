    "@microsoft/sp-tslint-rules": "1.9.1",
    "@microsoft/sp-webpart-workbench": "1.9.1",
    "@types/chai": "3.4.34",
    "@types/mocha": "2.2.38",
    "ajv": "~5.2.2",
    "concurrently": "^5.2.0",
    "cross-env": "7.0.2",
    "css-loader": "3.4.2",
    "css-modules-typescript-loader": "4.0.0",
